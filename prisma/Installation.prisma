model Installation {
    id              Int      @id @default(autoincrement())
    createdDate     DateTime @default(now())
    lastUpdatedDate DateTime @updatedAt
    sortOrder       Int      @default(0)

    fileSizeBytes     Int
    fileSizeGibiBytes Int
    path              String

    game_id Int

    game Game @relation(fields: [game_id], references: [id])
}
